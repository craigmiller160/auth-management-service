type Role {
    id: Int
    name: String!
    clientId: Int!
}

type Client {
    id: Int
    name: String!
    clientKey: String!
    clientSecret: String
    enabled: Boolean!
    allowClientCredentials: Boolean!
    allowPassword: Boolean!
    allowAuthCode: Boolean!
    accessTokenTimeoutSecs: Int!
    refreshTokenTimeoutSecs: Int!
    roles: [Role]!
}

type Query {
    clients: [Client]!
}
